{
   "content_scripts": [ {
      "js": [ "kmerged.user.js", "env/init/recorderFront.js"],
      "all_frames": true,
      "matches": [ 
        "http://*/*",
        "https://*/*"
        ]
   } ],
   "manifest_version" : 2,
   "version": "1.1.7.5",
   "name": "__MSG_ext_name_recorder__",
   "description": "__MSG_ext_description_recorder__",
   "icons": { 
        "32": "env/img/icon32x32.png",
        "44": "env/img/icon44x44.png",
        "128": "env/img/icon128x128.png" },
   "author" : "nradiowave",
   "default_locale" : "en",
   "permissions": [
        "downloads",
        "storage",
        "tabs",
        "unlimitedStorage",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
   "background": {
    "scripts": ["kmerged.bg.js", "env/init/background.js"],
    "persistent": true
   },  
   "options_ui": {
    "page": "env/html/recorderDownloader.html?tab=options",
    "open_in_tab": true
   },
   "browser_action": {
    "default_popup": "env/html/recorderPopup.html"
   },
   "web_accessible_resources": [
        "env/css/*.css",
        "env/pages/*.js",
        "env/profile/*.js",
        "env/dynamic/*.js"
    ]
}
