{
   "content_scripts": [ {
      "js": [ "kmerged.user.js", "init/init.js" ],
      "all_frames": true,
      "matches": [ 
        "http://*/*",
        "https://*/*"
        ]
   } ],
   "manifest_version" : 2,
   "version": "1.1.4.8",
   "name": "__MSG_ext_name__",
   "description": "__MSG_ext_description__",
   "icons": { 
        "32": "env/img/icon32x32.png",
        "44": "env/img/icon44x44.png",
        "128": "env/img/icon128x128.png" },
   "author" : "nradiowave",
   "default_locale" : "en",
   "permissions": [
        "downloads",
        "storage",
        "tabs",
        "unlimitedStorage",
        "<all_urls>"
    ],
   "background": {
    "scripts": ["kmerged.bg.js", "init/init.bg.js"],
    "persistent": false
   },  
   "options_ui": {
    "page": "env/pages/options.html",
    "open_in_tab": true
   },
   "web_accessible_resources": [
        "env/css/*.css",
        "env/profile/*.js",
        "env/dynamic/*.js"
    ]
}
